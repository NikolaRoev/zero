{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "zero",
  "identifier": "zero",
  "build": {
    "beforeDevCommand": "vite dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "vite build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
